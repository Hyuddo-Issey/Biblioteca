RewriteEngine On
RewriteBase /proyecto_mvc/public/

# No reescribir si es un archivo (im√°genes, css, js) o directorio real
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Mandar TODO lo que se escriba en la URL a la variable 'url'
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]